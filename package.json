  "version": "0.7.10",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "contributors": [
    "Raquel N Salva <raquelzssalva@gmail.com>",
    "Eugenio Arosteguy <eugenio.arosteguy@gmail.com>",
    "Emilio Platzer <emilioplatzer@gmail.com>"
  ],
  "repository": "codenautas/varcal",
  "license": "MIT",
  "main": "dist/server/app-varcal.js",
  "types": "dist/server/app-varcal.d.ts",
  "files": [
    "dist",
    "install"
  ],
  "dependencies": {
    "expre-parser": "~0.1.22",
    "operativos": "~0.7.11",
    "last-agg": "~1.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "~5.2.7",
    "mocha": "^6.2.2",
    "discrepances": "~0.2.4",
    "types.d.ts": "~0.4.9",
    "mixin-patch": "~0.3.1"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "x-test": "tsc || echo \"continue with errors\" && mocha --reporter spec --bail --check-leaks tests\\tests\\*.js",
    "test": "(npm run build || echo \"continue with errors\") && mocha --reporter spec --bail --check-leaks tests/tests/*.js",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks tests/tests/",
    "prepare": "npm run build",
    "build-strict": "tsc -p tsconfig-client.json && tsc -p tsconfig-server.json && mixin-patch",
    "build": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-server.json  || echo \"continue w/error\") && mixin-patch",
    "watch:buildS": "(tsc -p tsconfig-server.json --watch || echo \"continue w/error\")",
    "start": "node dist/server/server-varcal.js",
    "clink": "npm prune && npm run build && npm link operativos && npm run build",
    "publish-stack": "ncu -u && npm version patch && npm publish"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "lib",
    "ecmaVersion": 6
  }
}
